    "@angular/forms": "^4.3.0",
    "@angular/http": "^4.3.0",
    "@angular/platform-browser": "^4.3.0",
    "@angular/platform-browser-dynamic": "^4.3.0",
    "@angular/router": "^4.3.0",
    "@asymmetrik/angular2-leaflet": "^2.1.3",
    "@mediacomem/biosentiers-qrcode": "^2.1.2",
    "@types/leaflet": "^1.0.60",
    "@types/lodash": "^4.14.69",
    "angular2-moment": "^1.5.0",
    "bootstrap": "^3.3.7",
    "color": "^2.0.0",
    "core-js": "^2.4.1",
    "font-awesome": "^4.7.0",
    "leaflet": "^1.0.3",
    "lodash": "^4.17.4",
