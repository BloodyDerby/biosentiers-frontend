    "ngx-mydatepicker": "^2.0.4",
    "ngx-toastr": "^5.3.0",
    "qrcode": "^0.8.1",
    "rxjs": "^5.0.1",
    "ts-helpers": "^1.1.1",
    "zone.js": "^0.8.10"
  },
  "devDependencies": {
    "@angular/cli": "~1.2.1",
